.format { .major 0; .minor 5; };
.module { .name "Return"; .version 1 0 0; };

.code @code_main {
    .entry $ENTRY;
    .block $ENTRY () {
        %l_exit = const.i s32 0;
        ret %l_exit;
    };
};

.function @main () returns (s32) {
    .name "Main";
    .body defined @code_main;
};

.entry @main;
